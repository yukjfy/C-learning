void SMILE()
{
	printf("smile!");
}

int main(void)
{
	SMILE();
	SMILE();
	SMILE();
	printf("\n");
	SMILE();
	SMILE();
	printf("\n");
	SMILE();
	return 0;
}
//2022/1/24

int main()
{
	int i = 1;
	while (i <= 100)
	{
		if (i % 2 == 1)
		{
			printf("%d\n", i);
		}
		i++;
	}

	return 0;
}
