int main()  //计算1!+2!+...+10!
{
	int n = 10;
	long tem = 1;
	long sum = 0;
	int i = n;
	
	for (n=1; n <= 10; n++)
	{
		tem *= n;
		sum += tem;
	}
	printf("%d", sum);
	return 0;
}
